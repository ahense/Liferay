stanceId": [], "dataType": "string", "tooltip": {"en_US": ""}, "requireConfirmation": false, "displayStyle": "singleline", "fieldNamespace": "", "visibilityExpression": "", "ddmDataProviderInstanceOutput": [], "options": {}, "nativeField": false, "confirmationLabel": {"en_US": ""}, "fieldReference": "Text92029287", "placeholder": {"en_US": "An entity responsible for making contributions to the resource."}, "dataSourceType": "", "direction": ["vertical"]}, "defaultValue": {}, "fieldType": "text", "indexType": "keyword", "indexable": true, "label": {"en_US": "Contributor"}, "localizable": true, "name": "Text92029287", "nestedDataDefinitionFields": [], "readOnly": false, "repeatable": true, "required": false, "showLabel": true, "tip": {"en_US": "The guidelines for using names of persons or organizations as creators apply to contributors."}}, {"customProperties": {"labelAtStructureLevel": true, "confirmationErrorMessage": {"en_US": ""}, "hideField": false, "autocomplete": false, "ddmDataProviderInstanceId": [], "dataType": "string", "tooltip": {"en_US": ""}, "requireConfirmation": false, "displayStyle": "multiline", "fieldNamespace": "", "visibilityExpression": "", "ddmDataProviderInstanceOutput": [], "options": {}, "nativeField": false, "confirmationLabel": {"en_US": ""}, "fieldReference": "Text66620928", "placeholder": {"en_US": "The spatial or temporal topic of the resource, spatial applicability of the resource, or jurisdiction under which the resource is relevant."}, "dataSourceType": "", "direction": ["vertical"]}, "defaultValue": {}, "fieldType": "text", "indexType": "keyword", "indexable": true, "label": {"en_US": "Coverage"}, "localizable": true, "name": "Text66620928", "nestedDataDefinitionFields": [], "readOnly": false, "repeatable": true, "required": false, "showLabel": true, "tip": {"en_US": "Spatial topic and spatial applicability may be a named place or a location specified by its geographic coordinates. Temporal topic may be a named period, date, or date